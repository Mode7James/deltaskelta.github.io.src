{
  "name": "deltaskelta.github.io",
  "description": "Jeff Willette's developer blog",
  "version": "1.0.0",
  "author": "Jeff Willette <jrwillette88@gmail.com>",
  "dependencies": {
    "@babel/core": "^7.2.0",
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "gatsby": "^2.0.64",
    "gatsby-link": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-plugin-typography": "^2.2.2",
    "gatsby-source-filesystem": "^2.0.11",
    "gatsby-transformer-remark": "^2.1.15",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.6.1",
    "react-typography": "^0.16.18",
    "typography": "^0.16.18",
    "typography-theme-github": "^0.16.18"
  },
  "keywords": [
    "Jeff Willette",
    "deltaskelta"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --port 8001",
    "get-schema": "apollo service:download --endpoint http://localhost:8001/___graphql ./src/gatsby-schema.json",
    "gatsby-gen": "apollo client:codegen --addTypename --outputFlat --tagName=graphql --target=typescript --localSchemaFile=./src/gatsby-schema.json --includes=./src/pages/**/*.tsx ./src/gatsby-queries.d.ts",
    "gen": "yarn get-schema && yarn gatsby-gen",
    "format": "prettier --write \"src/**/*.ts*\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-dom": "16.0.11",
    "@types/react-helmet": "^5.0.7",
    "@types/typography": "^0.16.0",
    "prettier": "^1.15.3"
  }
}
