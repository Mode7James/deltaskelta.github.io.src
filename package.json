  "keywords": [
    "Jeff Willette",
    "deltaskelta"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --port 8001",
    "clean": "rm -r ./.cache",
    "get-schema": "apollo service:download --endpoint http://localhost:8001/___graphql ./gatsby-schema.json",
    "gatsby-gen": "apollo client:codegen --addTypename --outputFlat --tagName=graphql --target=typescript --localSchemaFile=./gatsby-schema.json  --includes='./{src,.cache}/**/*.{ts,tsx,js,jsx}' ./src/gatsby-queries.d.ts",
    "gen": "yarn get-schema && yarn gatsby-gen",
    "format": "prettier --write \"src/**/*.ts*\"",
    "test": "jest --coverage && yarn typecheck && yarn lint",
    "typecheck": "tsc --noEmit",
    "lint": "tslint --config ./tslint.json --fix '**/*.{ts,tsx}'"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-dom": "16.0.11",
    "@types/react-helmet": "^5.0.7",
    "prettier": "^1.15.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.0",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
