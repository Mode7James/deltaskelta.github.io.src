  "license": "MIT",
  "keywords": [
    "Jeff Willette",
    "deltaskelta"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --port 8001",
    "get-schema": "apollo service:download --endpoint http://localhost:8001/___graphql ./src/gatsby-schema.json",
    "gatsby-gen": "apollo client:codegen --addTypename --outputFlat --tagName=graphql --target=typescript --localSchemaFile=./src/gatsby-schema.json --includes=./src/**/*.tsx ./src/gatsby-queries.d.ts",
    "gen": "yarn get-schema && yarn gatsby-gen",
    "format": "prettier --write \"src/**/*.ts*\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-dom": "16.0.11",
    "@types/react-helmet": "^5.0.7",
    "prettier": "^1.15.3"
  }
}
